+#define	MACHINE_ARCH	"i386"
+#endif
+#define MID_MACHINE	MID_I386
+
+/* Endian-ness */
+#ifndef BIG_ENDIAN
+#define BIG_ENDIAN	4321
+#endif
+#ifndef LITTLE_ENDIAN
+#define LITTLE_ENDIAN	1234
+#endif
+
+#ifndef BYTE_ORDER
+#define BYTE_ORDER	LITTLE_ENDIAN
+#endif
