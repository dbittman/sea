+	act.sa_handler = handler;
+	if (sigemptyset (&act.sa_mask) < 0 || sigaddset (&act.sa_mask, sig) < 0)
+		return SIG_ERR;
+	act.sa_flags = sigismember (&_sigintr, sig) ? 0 : SA_RESTART;
+	if (sigaction (sig, &act, &oact) < 0)
+		return SIG_ERR;
