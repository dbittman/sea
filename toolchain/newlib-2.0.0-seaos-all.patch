+#define ptoa(x)			((unsigned)(x) << PAGE_SHIFT)
+
+#define i386_btop(x)		((unsigned)(x) >> PAGE_SHIFT)
+#define i386_ptob(x)		((unsigned)(x) << PAGE_SHIFT)
+
+#define	pgtok(x)		((x) * (PAGE_SIZE / 1024))
