+	int ret = syscall(SYS_POSFSSTAT, fd, (scarg_t)buf, 0, 0, 0);
+	if(ret < 0) {
+		errno = -ret;
+		return -1;
+	}
+	return ret;
+}
+
+int fstat(int fd, struct stat *buf)
+{
+	struct kstat k;
+	int ret= syscall(SYS_FSTAT, fd, (scarg_t)&k, 0, 0, 0);
+	if(ret < 0)
+	{
+		errno = -ret;
+		return -1;
+	}
+	__internal_convert_kstat_stat(&k, buf);
+	return ret;
+}
+
+int _fstat(int f, struct stat *b)
+{
+	return fstat(f, b);
+}
+
diff -uprN newlib-2.0.0/newlib/libc/sys/seaos/sync.c newlib-2.0.0-sea/newlib/libc/sys/seaos/sync.c
