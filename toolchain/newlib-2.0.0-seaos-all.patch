+        ns_kt_rsa = 1,          /* key type RSA/MD5 */
+        ns_kt_dh  = 2,          /* Diffie Hellman */
+        ns_kt_dsa = 3,          /* Digital Signature Standard (MANDATORY) */
+        ns_kt_private = 254     /* Private key type starts with OID */
+} ns_key_types;
