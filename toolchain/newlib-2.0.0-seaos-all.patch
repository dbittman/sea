+/* id.c - deal with various IDs for things. uid, gid, etc. Many of these
+ * functions don't return errors ever. */
+
+#include "ksyscall.h"
+#include <signal.h>
+#include <stdio.h>
+#include <errno.h>
+int getpid()
+{
+	int ret = syscall(SYS_GETPID, 0, 0, 0, 0, 0);
+	if(ret < 0) {
+		errno = -ret;
+		return -1;
+	}
+	return ret;
+}
+int _getpid()
+{
+	int ret = syscall(SYS_GETPID, 0, 0, 0, 0, 0);
+	if(ret < 0) {
+		errno = -ret;
+		return -1;
+	}
+	return ret;
+}
+
+uid_t get_uid()
+{
+	return syscall(SYS_GETUID, 0,0,0,0,0);
+}
+
+uid_t getuid()
+{
+	return get_uid();
+}
+
+uid_t geteuid()
+{
+	return syscall(SYS_GETEUID, 0,0,0,0,0);
+}
+
+int set_uid(int n)
+{
+	int ret = syscall(SYS_SETUID, n,0,0,0,0);
+	if(ret < 0) {
+		errno = -ret;
+		return -1;
+	}
+	return ret;
+}
+
+int setreuid(uid_t r, uid_t e)
+{
+	int ret = syscall(SYS_SETUID, r,e,0,0,0);
+	if(ret < 0) {
+		errno = -ret;
+		return -1;
+	}
+	return ret;
+}
+
+int setuid(uid_t n)
+{
+	return set_uid(n);
+}
+
+int seteuid(uid_t n)
+{
+	int ret = syscall(SYS_SETEUID, n,0,0,0,0);
+	if(ret < 0) {
+		errno = -ret;
+		return -1;
+	}
+	return ret;
+}
+
+
+
+
+
+int getgrgid()
+{
+	return 0;
+}
+
+int setpgid(pid_t a, pid_t b)
+{
+	int ret = syscall(SYS_SETPGID, a, b, 0, 0, 0);
+	if(ret < 0)
+	{
+		errno=-ret;
+		return -1;
+	}
+	return ret;
