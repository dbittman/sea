+int nice(int inc)
+{
+	/* which, who, value, flags */
+	int ret = syscall(SYS_NICE, 0, 0, inc, 0, 0);
+	if(ret < 0)
+	{
+		errno = -ret;
+		return -1;
+	}
+	return ret;
+}
