diff -uprN newlib-2.0.0/newlib/libc/sys/seaos/sysc.c newlib-2.0.0-sea/newlib/libc/sys/seaos/sysc.c
--- newlib-2.0.0/newlib/libc/sys/seaos/sysc.c	1969-12-31 16:00:00.000000000 -0800
+++ newlib-2.0.0-sea/newlib/libc/sys/seaos/sysc.c	2014-03-03 22:44:44.370561370 -0800
@@ -0,0 +1,11 @@
+#include "ksyscall.h"
+#include <sys/stat.h>
+#include <errno.h>
+scarg_t syscall(int num, scarg_t a, scarg_t b, scarg_t c, scarg_t d, scarg_t e)
+{
+	errno = 0;
+	scarg_t x;
+	__asm__ __volatile__("int $0x80":"=a"(x):"0" (num), "b" ((scarg_t)a),
+			 "c" ((scarg_t)b), "d" ((scarg_t)c), "S" ((scarg_t)d), "D" ((scarg_t)e));
+	return x;
+}
diff -uprN newlib-2.0.0/newlib/libc/sys/seaos/termios.c newlib-2.0.0-sea/newlib/libc/sys/seaos/termios.c
--- newlib-2.0.0/newlib/libc/sys/seaos/termios.c	1969-12-31 16:00:00.000000000 -0800
+++ newlib-2.0.0-sea/newlib/libc/sys/seaos/termios.c	2014-03-03 22:44:44.370561370 -0800
@@ -0,0 +1,56 @@
+/* libc/sys/linux/termios.c - Terminal control */
+/* Written 2000 by Werner Almesberger */
+
+#include <errno.h>
+#include <sys/types.h>
+#include <sys/termios.h>
+#include <sys/ioctl.h>
+
+int tcgetattr(int fd,struct termios *termios_p)
+{
+	return ioctl(fd,TCGETS,termios_p);
+}
+
+int tcsetattr(int fd, int optional_actions, const struct termios *termios_p)
+{
+	int cmd;
+	switch (optional_actions) {
+		case TCSANOW:
+			cmd = TCSETS;
+			break;
+		case TCSADRAIN:
+			cmd = TCSETSW;
+			break;
+		case TCSAFLUSH:
+			cmd = TCSETSF;
+			break;
+		default:
+			errno = EINVAL;
+			return -1;
+	}
+	return ioctl(fd,cmd,termios_p);
+}
+
+pid_t tcgetpgrp(int fd)
+{
+	int p;
+	if (ioctl(fd,TIOCGPGRP,&p) < 0)
+		return (pid_t)-1;
+	return (pid_t)p;
+}
+
+int tcsetpgrp(int fd, pid_t pid)
+{
+	int p = (int)pid;
+	return ioctl(fd,TIOCSPGRP,&p);
+}
+
+int tcflow (int fd, int action)
+{
+	return ioctl (fd, TCXONC, action);
+}
+
+int tcflush (int fd, int queue_selector)
+{
+	return ioctl (fd, TCFLSH, queue_selector);
+}
