diff -uprN newlib-2.0.0/newlib/libc/sys/seaos/machine/i386/dl-machine.h newlib-2.0.0-sea/newlib/libc/sys/seaos/machine/i386/dl-machine.h
--- newlib-2.0.0/newlib/libc/sys/seaos/machine/i386/dl-machine.h	1969-12-31 16:00:00.000000000 -0800
+++ newlib-2.0.0-sea/newlib/libc/sys/seaos/machine/i386/dl-machine.h	2014-03-03 22:44:44.360561494 -0800
@@ -0,0 +1,420 @@
